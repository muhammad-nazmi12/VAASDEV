{
    "version":2,
    "builds":[
        {
            "src":"manage.py",
            "use":"@vercel/python"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"127.0.0.1:8000/$1"
        },
        {
            "src":"/static/(.*)",
            "dest":"127.0.0.1:8000/static/$1"
        }
    ]
}